<p id="notice"><%= notice %></p>

<p>
  <strong>Outcome:</strong>
  <%= @combo.outcome %>
</p>

<p>
  <strong>Card spots count:</strong>
  <%= @combo.card_spots_count %>
</p>

<ul>
  <% @combo.card_spots.order(:ord).each do |cs| %>
    <li> 
      <%=cs.ord%>)
      <%=cs.card.name%>
    </li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_combo_path(@combo) %> |
<%= link_to 'Back', combos_path %>
